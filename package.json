{
  "name": "messanger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "start": "vite build && vite preview --port 3000",
    "prebuild": "tsc --noEmit",
    "eslint": "eslint . && tsc --noEmit",
    "eslint-fix": "eslint . --fix",
    "test": "jest",
    "prepare": "husky",
    "pre-commit": "npm test && npm run eslint"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test eslint"
    }
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.8",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@jest/globals": "29.7.0",
    "@types/uuid4": "2.0.3",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "10.4.20",
    "babel-jest": "29.7.0",
    "chai": "5.1.2",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "husky": "8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "25.0.1",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.49",
    "postcss-load-config": "6.0.1",
    "postcss-nested": "6.2.0",
    "prettier": "3.3.3",
    "stylelint": "16.10.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.11.0",
    "vite": "5.4.8",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-handlebars": "2.0.0"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "uuid4": "^2.0.3"
  }
}
